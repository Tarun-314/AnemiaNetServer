{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/conjunctiva",
      "dest": "app.py"
    },
    {
      "src": "/finger_nail",
      "dest": "app.py"
    },
    {
      "src": "/palm",
      "dest": "app.py"
    },
    {
      "src": "/final",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  },
  "functions": {
    "app": {
      "runtime": "python3.8",
      "memory": 1024
    }
  },
  "dependencies": {
    "flask": "3.0.2",
    "flask-cors": "4.0.0",
    "gunicorn": "*",
    "joblib": "1.3.2",
    "keras": "2.15.0",
    "numpy": "1.23.5",
    "scikit-learn": "1.2.2",
    "tensorflow": "2.15.0",
    "opencv-python": "4.9.0.80",
    "Pillow": "9.4.0"
  }
}
