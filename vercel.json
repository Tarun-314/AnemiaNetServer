{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/conjunctiva",
      "dest": "app.py"
    },
    {
      "src": "/finger_nail",
      "dest": "app.py"
    },
    {
      "src": "/palm",
      "dest": "app.py"
    },
    {
      "src": "/final",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "app.py"
  }
}
